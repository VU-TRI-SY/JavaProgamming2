	public int min(int[] arr) {
		
		return 0;
	}
	
	public boolean isAscSorted(int[] arr) {
		
		return true;
	}
	
	public int length(char[] arr) {
		int len = 0;
	    for (char c : arr) {
	        if (c == '\u0000') {
	            break;
	        }
	        len++;
	    }
	    return len;
	}
	
	public double median(double[] arr) {
		
		return 0.0;
	}
	
