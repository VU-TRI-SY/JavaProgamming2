package ex12_3;

import java.util.Scanner;

public class Ex4 {
	Scanner sc = new Scanner(System.in);
	int N;
	System.out.print("Enter the number of input integers: ");
	N = sc.nextInt();
	if(N <= 0) {
		System.out.println("Invalid input!");
		sc.close();
		return;
	}
	
	System.out.println("Enter the N integers: ");
}
